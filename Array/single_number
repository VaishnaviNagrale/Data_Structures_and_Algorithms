class Solution {
public:
    int singleNumber(vector<int>& nums) {
        /* linear runtime complexity and use only constant extra space : means only optimal solution */
       //Optimal Approch : XOR
	   int XOR=0;
	   for(int i=0;i<nums.size();i++){
	       XOR=XOR^nums[i];
	   }
	   return XOR;
    }
};
